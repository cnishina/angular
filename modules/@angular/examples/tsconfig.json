{
  "compilerOptions": {
    "baseUrl": ".",
    "stripInternal": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "../../../dist/examples",
    "paths": {
      "@angular/*": ["../../../dist/packages-dist/*"],
      "rxjs/*": ["../../../node_modules/rxjs/*"],
      "protractor": ["../../../node_modules/protractor/built/index.d.ts"],
      "protractor/globals": ["../../../node_modules/protractor/globals.d.ts"],
      "selenium-webdriver": ["../../../node_modules/protractor/typings/globals/selenium-webdriver/index.d.ts"]
    },
    "rootDir": ".",
    "sourceMap": true,
    "inlineSources": true,
    "target": "es5",
    "lib": ["es2015", "dom"],
    "skipLibCheck": true,
    "types": ["jasmine", "node", "selenium-webdriver"]
  },
  "include": [
    "./_common/*.ts",
    "./**/module.ts",
    "./**/test/*.ts",
    "./**/e2e_test/*.ts",
    "../../../node_modules/zone.js/dist/zone.js.d.ts",
    "../../system.d.ts"
  ]
}
